	rdfs:label "Zakelijk recht identificatie"@nl;
	skos:prefLabel "Zakelijk recht identificatie"@nl;
	skos:definition "Een Zakelijk recht identificatie is de unieke identificatie van een zakelijk recht in de BRK."@nl;
	skos:related kad:ZakelijkRecht;
.

