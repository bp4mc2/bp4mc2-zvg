#	skos:exactMatch brk:Bijwerking_Basisregistratie_Kadaster; -> nog te definiÃ«ren
	rdfs:label "Bijwerking basisregistratie Kadaster";
	skos:prefLabel "Bijwerking basisregistratie Kadaster";
	dcterms:source <http://wetten.overheid.nl/jci1.3:c:BWBR0004541&hoofdstuk=4&titeldeel=1&afdeling=1&artikel=53>;
	skoslex:object kad:BasisregistratieKadaster;
