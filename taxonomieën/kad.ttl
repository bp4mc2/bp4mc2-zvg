	skos:exactMatch brk:Inschrijving;
	rdfs:label "Inschrijving openbare registers"@nl;
	skos:prefLabel "Inschrijving openbare registers"@nl;
	skos:definition "Inschrijving openbare registers is het proces waarbij de ter inschrijving aangeboden stukken worden opgenomen in de openbare registers."@nl;
	rdfs:comment "Inschrijving openbare registers is het opnemen van een akte in de Openbare Registers."@nl;
	dcterms:source <http://wetten.overheid.nl/jci1.3:c:BWBR0004541&hoofdstuk=2&titeldeel=1&afdeling=1&artikel=8>;
	skoslex:object not:AfschriftAkte;
