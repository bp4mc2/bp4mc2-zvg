	skos:inScheme <http://taxonomie.zorgeloosvastgoed.nl/id/ConceptualFramework/zv>; 
	skos:inScheme <http://taxonomie.zorgeloosvastgoed.nl/id/ConceptualSystem/ZorgeloosVastgoed>; 
	rdfs:label "Aangifte overdrachtsbelasting"@nl;
	skos:prefLabel "Aangifte overdrachtsbelasting"@nl;
	skos:definition "Aangifte voor de verschuldigde overdrachtsbelasting bij de belastingdienst."@nl;
	skos:editorialNote "Gebeurt gelijktijdig met het registreren van de overdrachtsbelasting."@nl;
