@prefix elmo: <http://dotwebstack.org/def/elmo#> .
@prefix elmo-rt: <http://dotwebstack.org/def/elmo/result-type/>.
@prefix config: <http://dotwebstack.org/configuration/>.

GRAPH config:Theatre {

  config:HomeData a elmo:InformationProduct;
    elmo:backend config:DefaultBackend;
    elmo:query """
      prefix elmo1: <http://bp4mc2.org/elmo/def#>
      prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>
      CONSTRUCT {
        [] rdfs:label "Zorgeloos vastgoed"@nl;
          elmo1:html '''
            <a href="/wie">
              <button class="btn btn-primary">
                <img src="/assets/images/actor.png" width="100"/>
                <p>Wie?</p>
              </button>
            </a>
            <a href="/wat">
              <button class="btn btn-primary">
                <img src="/assets/images/object.png" width="100"/>
                <p>Wat?</p>
              </button>
            </a>
            <a href="/hoe">
              <button class="btn btn-primary">
                <img src="/assets/images/act.png" width="100"/>
                <p>Hoe?</p>
              </button>
            </a>
            <a href="/met">
              <button class="btn btn-primary">
                <img src="/assets/images/agent.png" width="100"/>
                <p>Met wie?</p>
              </button>
            </a>
          '''
        .
      }
      WHERE {}
    """
  .

}
