@prefix elmo: <http://dotwebstack.org/def/elmo#> .
@prefix config: <http://dotwebstack.org/configuration/>.

GRAPH config:Theatre {

  config:MetEndpoint a elmo:Endpoint;
    elmo:pathPattern "/met";
    elmo:stage config:Stage;
    elmo:getRepresentation config:Met;
  .

  config:Met a elmo:Representation;
    elmo:stage config:Stage;
    elmo:informationProduct config:MetData;
    elmo:appearance config:MetAppearance;
    elmo:contains config:Menu;
  .

  config:MetAppearance a elmo:TableAppearance;
    elmo:fragment [
      elmo:appliesTo "begrip";
      rdfs:label "Begrip"@nl;
    ];
    elmo:fragment [
      elmo:appliesTo "def";
      rdfs:label "Definitie"@nl;
    ];
  .

}
